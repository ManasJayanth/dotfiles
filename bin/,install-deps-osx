#!/bin/bash

brew update

brew install \
    coreutils \
    git \
    python \
    python3 \
    tmux \
    tree \
    irssi \
    ack \
    wget \
    bash-completion \
    macvim \
    emacs \
    postgresql \
    fasd

# Required to allow tmux to merge w/osx clipboard
brew install reattach-to-user-namespace

if [ ! -d ~/sh ]; then
    echo "~/sh not found, creating..."
    mkdir -v ~/sh
fi
